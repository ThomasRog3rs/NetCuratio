<!DOCTYPE html>
<html lang="zxx">
<head>
    <title>Net Curatio - Websites that are out of this world</title>
    <meta charset="UTF-8">
    <meta name="description" content="Cloud 83 - hosting template ">
    <meta name="keywords" content="cloud, hosting, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Favicon -->
    <link href="@Url.Content("~/Content/img/favicon.png")" rel="shortcut icon" />

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,500,500i,600,600i,700,700i" rel="stylesheet">


    <!-- Stylesheets -->
    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/flaticon.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/owl.carousel.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/animate.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/style.css")" rel="stylesheet" />

    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header section -->
    <header class="header-section">
        <a href="@Url.Action("Index", "Home")" class="site-logo fw-title text-white font-weight-bold"><img src="@Url.Content("~/Content/img/section-title-icon.png")" alt="logo"> Net Curatio</a>
        <div class="nav-switch">
            <i class="fa fa-bars"></i>
        </div>
        <div class="nav-warp mr-5">
            <ul class="main-menu">
                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                <li><a href="@Url.Action("About", "Home")">About us</a></li>
                <li><a href="@Url.Action("Services", "Home")">Services</a></li>
                <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
            </ul>
        </div>
    </header>
    <!-- Header section end -->

    @RenderBody()

    <!-- Footer top section -->
    <!-- Milestones section -->
    <section class="milestones-section set-bg" data-setbg="@Url.Content("~/Content/img/milestones-bg.jpg")">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="fab fa-google mb-2 mt-2"></i>
                        <p>SEO optimised</p>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="far fa-clock mb-2 mt-2"></i>
                        <p>24 Hours of support</p>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="fas fa-server mb-2 mt-2"></i>
                        <p>Free Hosting</p>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="fas fa-shield-alt mb-2 mt-2"></i>
                        <p>SSL Certificates</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Milestones section end -->
    <!-- Footer top section end -->
    <!-- Footer section -->
    <footer class="footer-section">
        <div class="container">
            <ul class="footer-menu">
                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                <li><a href="@Url.Action("About", "Home")">About us</a></li>
                <li><a href="@Url.Action("Services", "Home")">Services</a></li>
                <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
            </ul>
            <div class="copyright">
                Copyright &copy;
                @DateTime.Now.Year All rights reserved | Net Curatio
            </div>
        </div>
    </footer>
    <!-- Footer top section end -->
    <!--====== Javascripts & Jquery ======-->
    <script src="@Url.Content("~/Scripts/js/jquery-3.2.1.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/owl.carousel.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/circle-progress.min.js")"></script>
    <script src="@Url.Content("~/Scripts/js/main.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
    <script src="https://kit.fontawesome.com/c59cb3ca97.js" crossorigin="anonymous"></script>

</body>
</html>