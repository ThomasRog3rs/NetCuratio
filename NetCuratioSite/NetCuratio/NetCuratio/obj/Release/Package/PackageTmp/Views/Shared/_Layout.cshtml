<!DOCTYPE html>
<html lang="zxx">
<head>
    <title>Net Curatio - Websites that are out of this world</title>
    <meta charset="UTF-8">
    <meta name="description" content="Beautiful websites, expertly made. Get your business online today for as little as £19.99/month. That's half the price of your average monthly phone bill. Not bad, hey!">
    <meta name="keywords" content="cloud, hosting, creative, html, website, design, development, responsive, mobile, modern">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Favicon -->
    <link href="@ViewBag.Root/Content/img/favicon.png" rel="shortcut icon" type="image/png"/>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,500,500i,600,600i,700,700i" rel="stylesheet" type="text/css">


    <!-- Stylesheets -->
    <link href="@ViewBag.Root/Content/css/bootstrap.min.css" rel="stylesheet" type="text/css" runat="server"/>
    <link href="@ViewBag.Root/Content/css/font-awesome.min.css" rel="stylesheet" type="text/css" runat="server"/>
    <link href="@ViewBag.Root/Content/css/flaticon.css" rel="stylesheet" type="text/css" runat="server"/>
    <link href="@ViewBag.Root/Content/css/owl.carousel.min.css" rel="stylesheet" type="text/css" runat="server"/>
    <link href="@ViewBag.Root/Content/css/animate.css" rel="stylesheet" type="text/css" runat="server"/>
    <link href="@ViewBag.Root/Content/css/style.css" rel="stylesheet" type="text/css" runat="server"/>

    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header section -->
    <header class="header-section">
        <a href="@Url.Action("Index", "Home")" class="site-logo fw-title text-white font-weight-bold"><img src="@ViewBag.Root/Content/img/section-title-icon.png" loading="lazy"  alt="logo" typeof="image/png"> Net Curatio</a>
        <div class="nav-switch">
            <i class="fa fa-bars"></i>
        </div>
        <div class="nav-warp mr-5">
            <ul class="main-menu">
                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                <li><a href="@Url.Action("About", "Home")">About us</a></li>
                <li><a href="@Url.Action("Services", "Home")">Services</a></li>
                <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
            </ul>
        </div>
    </header>
    <!-- Header section end -->

    @RenderBody()
    @* WhatsApp chat *@
    <script src="https://apps.elfsight.com/p/platform.js" defer></script>
    <div class="elfsight-app-5b67594c-3816-49a4-b876-c828c520a801"></div>
    @* Cookies *@
    <script src="https://apps.elfsight.com/p/platform.js" defer></script>
    <div class="elfsight-app-ca513ca0-68a0-4677-b078-6c0947f85fe5"></div>
    <!-- Footer top section -->
    <!-- Milestones section -->
    <section class="milestones-section set-bg" data-setbg="@ViewBag.Root/Content/img/milestones-bg.jpg">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="fab fa-google mb-2 mt-2"></i>
                        <p>SEO optimised</p>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="far fa-clock mb-2 mt-2"></i>
                        <p>24 Hours of support</p>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="fas fa-server mb-2 mt-2"></i>
                        <p>Free Hosting</p>
                    </div>
                </div>
                <div class="col-md-3 col-sm-6">
                    <div class="milestone-item text-white">
                        <i class="fas fa-shield-alt mb-2 mt-2"></i>
                        <p>SSL Certificates</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Milestones section end -->
    <!-- Footer top section end -->
    <!-- Footer section -->
    <footer class="footer-section">
        <div class="container">
            <ul class="footer-menu">
                <li><a href="@Url.Action("Index", "Home")">Home</a></li>
                <li><a href="@Url.Action("About", "Home")">About us</a></li>
                <li><a href="@Url.Action("Services", "Home")">Services</a></li>
                <li><a href="@Url.Action("Contact", "Home")">Contact</a></li>
            </ul>
            <div class="copyright">
                Copyright &copy;
                @DateTime.Now.Year All rights reserved | Net Curatio <img src="@ViewBag.Root/Content/img/favicon.png" alt="Logo" loading="lazy"  style="max-height: 30px" typeof="image/png"/>
            </div>
        </div>
    </footer>

    <!-- Footer top section end -->
    <!--====== Javascripts & Jquery ======-->
    <script src="@ViewBag.Root/Scripts/js/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="@ViewBag.Root/Scripts/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="@ViewBag.Root/Scripts/js/owl.carousel.min.js" type="text/javascript"></script>
    <script src="@ViewBag.Root/Scripts/js/circle-progress.min.js" type="text/javascript"></script>
    <script src="@ViewBag.Root/Scripts/js/main.js" type="text/javascript"></script>
    <script src="@ViewBag.Root/Scripts/jquery.validate.min.js" type="text/javascript"></script>
    <script src="@ViewBag.Root/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    <script src="https://kit.fontawesome.com/c59cb3ca97.js" crossorigin="anonymous" type="text/javascript"></script>

</body>
</html>